{
  "name": "TranwallMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "type-check": "tsc --build plugins/typescript/template/tsconfig.json",
    "podinstall": "cd ios && pod install && cd ..",
    "reinstall-pods": "rm -rf ios/Pods && yarn podinstall",
    "reinstall-modules": "watchman watch-del-all && rm -rf ./node_modules && rm -rf /tmp/metro-* && yarn install",
    "reinstall": "yarn reinstall-modules && yarn reinstall-pods",
    "check-format": "./node_modules/.bin/prettier --check './src/**/*.{js,ts,tsx,json}'",
    "lint": "eslint './src/**/*.{js,ts,tsx,json}'",
    "format": "./node_modules/.bin/prettier --write './src/**/*.{js,ts,tsx,json}' && yarn lint --fix",
    "localserver": "cd apolloserver && node index.js && cd ..",
    "format-server": "./node_modules/.bin/prettier --write './apolloserver/**/*.{js,ts,tsx,json}' && eslint './apolloserver/**/*.{js,ts,tsx,json}' --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@gorhom/bottom-sheet": "^4.1.3",
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/core": "^6.1.0",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.7",
    "@reduxjs/toolkit": "^1.6.1",
    "@thecodingmachine/redux-toolkit-wrapper": "2.0.1",
    "@types/react-native-snap-carousel": "^3.8.4",
    "appcenter": "^4.3.0",
    "appcenter-analytics": "^4.3.0",
    "appcenter-crashes": "^4.3.0",
    "axios": "^0.21.4",
    "date-fns": "^2.25.0",
    "graphql": "^15.6.0",
    "i18next": "^20.6.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-i18next": "^11.12.0",
    "react-native": "0.65.1",
    "react-native-flipper": "^0.103.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.4",
    "react-native-reanimated": "^2.2.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^12.1.1",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-flipper": "^2.0.0",
    "redux-persist": "^6.0.0",
    "tailwind-react-native-classnames": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/fbemitter": "^2.0.32",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/react": "^17.0.24",
    "@types/react-native": "^0.65.1",
    "@types/react-redux": "^7.1.18",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "prettier": "^2.4.1",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./jest.setup.js"
    ],
    "timers": "fakfe",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!@react-native|react-native)"
    ]
  }
}
