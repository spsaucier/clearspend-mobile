{
  "name": "ClearSpendMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "check-format": "./node_modules/.bin/prettier --check './src/**/*.{js,ts,tsx,json}'",
    "devices-android": "adb devices",
    "devices-ios": "xcrun xctrace list devices",
    "format-server": "yarn prettier-server && yarn lint-server",
    "format": "yarn prettier && yarn lint --fix",
    "generate": "node ./scripts/generateTypes.ts && git add ./src/generated/capital.ts",
    "ios": "react-native run-ios",
    "lint-server": "eslint './restserver/**/*.{js,ts,tsx,json}' --fix",
    "lint": "eslint './src/**/*.{js,ts,tsx,json}'",
    "podinstall": "cd ios && pod install && cd ..",
    "prettier-server": "./node_modules/.bin/prettier --write './restserver/**/*.{js,ts,tsx,json}'",
    "prettier": "./node_modules/.bin/prettier --write './src/**/*.{js,ts,tsx,json}'",
    "reinstall-modules": "watchman watch-del-all && rm -rf ./node_modules && rm -rf /tmp/metro-* && yarn install",
    "reinstall-pods": "rm -rf ios/Pods && yarn podinstall",
    "reinstall": "yarn reinstall-modules && yarn reinstall-pods",
    "restserver": "cd restserver && node index.js && cd ..",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "type-check": "tsc --build plugins/typescript/template/tsconfig.json"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.1.3",
    "@react-native-clipboard/clipboard": "^1.9.0",
    "@react-native-community/blur": "^3.6.0",
